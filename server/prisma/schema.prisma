generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id          String        @id @default(uuid()) @map("_id") @db.ObjectId
  email       String        @unique
  name        String
  EnteredData EnteredData[]
}

model EnteredData {
  id     String @id @default(uuid()) @map("_id") @db.ObjectId
  lekh   String
  user   User   @relation(fields: [userId], references: [id])
  userId String @db.ObjectId
}
